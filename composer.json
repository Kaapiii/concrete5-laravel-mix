{
  "name": "kaapiii/concrete5-laravel-mix",
  "description": "Add versioned Laravel Mix files (based on your mix-manifest.js) to concrete5 themes and packages",
  "type": "library",
  "minimum-stability": "dev",
  "keywords": [
    "concrete5",
    "laravel-mix"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Markus Liechti",
      "email": "kaapiii@hotmail.com"
    }
  ],
  "require": {
    "php": "^7.2"
  } ,
  "require-dev": {
      "phpunit/phpunit": "^8.0|^9.0"
  },
  "autoload": {
    "psr-4": {
      "Kaapiii\\Concrete5\\LaravelMix": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Kaapiii\\Concrete5\\LaravelMix\\Test": "tests"
    }
  },
  "scripts": {
    "test": "vendor/bin/phpunit",
    "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
  }
}
